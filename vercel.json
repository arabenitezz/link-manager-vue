{
  "version": 2,
  "builds": [
    {
      "src": "front/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "headers": { 
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "/(.*)\\.js$",
      "headers": { 
        "content-type": "application/javascript" 
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
  